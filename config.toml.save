baseURL = "https://l1ge.github.io"

title = "l1ge's cabin"
languageCode = "en-us"
defaultContentLanguage = "en-us"

theme = "axiom"

paginate = 2
canonifyURLs = false
summaryLength = 40
rssLimit = 100
enableEmoji = true
enableRobotsTXT = true
pluralizelisttitles = false

[permalinks]
posts = "/:filename"

[params]
mainSections = ["posts"]
copyrightYear = 2020
# Go Reference time: Mon Jan 2 15:04:05 MST 2006
dateFormat = "Jan 2, 2006 3:04PM"

[params.brand]
domain = "l1ge.github.io"
email = "l1ge@protonmail.com"

[params.social]
github = "https://github.com/l1ge"
twitter = "https://twitter.com/the_l1ge"
footer = ["twitter", "1024github"]

[params.services]
analyticsId = ""
adwordsId = ""
adsenseId = ""
adsenseAdSlotId = ""
adsenseLazy = false
adwordsConversionId = ""
adwordsConversionValue = 0
facebookApp = ""
facebookPixel = ""

[params.logo]
inline = true


[params.image]
width = "2048"
height = "1024"
faviconIco = "favicon.ico"
faviconAlt = "image/brand/favicon.png"
icon1To1 = "image/brand/icon-1-1.png"
icon2To1 = "image/brand/icon-2-1.png"
default = "image/page-default.webp"

[params.font]
type = ""
path = ""
files = []

[params.cdn]
provider = "local"
type = "image"

[outputs]
home = ["HTML", "ATOM"]
section = ["HTML"]
taxonomy = ["HTML"]
taxonomyTerm = ["HTML"]

[outputFormats]
[outputFormats.ATOM]
name = "ATOM"
baseName = "feed"
mediaType = "application/atom+xml"

[mediaTypes]
[mediaTypes."application/atom+xml"]
suffixes = ["atom"]

[markup]
[markup.goldmark]
[markup.goldmark.extensions]
definitionList = false
footnote = true
linkify = false
strikethrough = false
table = true
taskList = false
typographer = false
[markup.goldmark.renderer]
unsafe = true
[markup.goldmark.parser]
attribute = false
autoHeadingID = false
[markup.highlight]
codeFences = false
noClasses = false
tabWidth = 2

[menu]
[[menu.main]]
url = "/posts/"
name = "Posts"
weight = 1

[[menu.main]]
url = "/contact/"
name = "Contact"
weight = 2

[[menu.foot]]
url = "/posts/"
name = "Posts"
weight = 1

[[menu.foot]]
url = "/contact/"
name = "Contact"
weight = 2


[languages]
[languages."en-us"]
weight = 1
languageName = "US English"
